{
  "manifest_version": 2,
  "name": "Offi",
  "description": "Link between logs.tf and ETF2L match pages",
  "version": "1.0.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/matches/*"
      ],
      "js": [
        "dist/etf2l_match_page.js"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/forum/user/*"
      ],
      "js": [
        "dist/etf2l_player_page.js"
      ],
      "css": [
        "styles.css"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://logs.tf/*"
      ],
      "js": [
        "dist/logstf.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "styles.css"
]
}
