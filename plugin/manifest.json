{
  "manifest_version": 3,
  "name": "Offi",
  "description": "etf2l.org QoL extension",
  "version": "2.0.4",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/matches/*"
      ],
      "js": [
        "dist/etf2l_match_page.js"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/forum/user/*"
      ],
      "js": [
        "dist/etf2l_player_page.js"
      ],
      "css": [
        "styles.css"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/teams/*"
      ],
      "js": [
        "dist/etf2l_team_page.js"
      ],
      "css": [
        "styles.css"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://logs.tf/*"
      ],
      "js": [
        "dist/logstf.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "web_accessible_resources": [{
      "resources": ["styles.css"],
      "matches": ["https://etf2l.org/*", "https://logs.tf/*"]
    }]
}
