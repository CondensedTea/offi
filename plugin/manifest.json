{
  "manifest_version": 3,

  "name": "Offi",
  "description": "Find logs for ETF2L officials",
  "version": "0.1",

  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://etf2l.org/matches/*"],
      "js": ["dist/index.js"]
    }
  ],
  "host_permissions": [
    "https://stg.lemontea.dev/offi/match/*"
  ],
  "permissions": [
    "activeTab",
    "declarativeContent"
  ]
}
