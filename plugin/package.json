    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
