{
  "name": "offi",
  "version": "4.0.0",
  "scripts": {
    "build_ff": "cp firefox_manifest.json manifest.json && parcel build manifest.json --dist-dir build/firefox --no-optimize --config @parcel/config-webextension && rm manifest.json",
    "build_c": "cp chrome_manifest.json manifest.json && parcel build manifest.json --dist-dir build/chrome --no-optimize --config @parcel/config-webextension && rm manifest.json",
    "lint": "eslint src/*.ts"
  },
  "dependencies": {
    "@kocal/web-extension-library": "^1.2.1",
    "@parcel/config-webextension": "2.13.0",
    "@parcel/core": "2.13.0",
    "axios": "^1.7.7",
    "parcel": "2.13.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^9.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "process": "^0.11.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "webextension-polyfill": "^0.12.0"
  },
  "repository": "github.com/CondensedTea/offi",
  "author": "CondensedTea",
  "license": "MIT"
}
