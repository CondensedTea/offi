{
  "manifest_version": 2,
  "name": "Offi",
  "description": "tf2 competitive QoL extension",
  "version": "3.1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/matches/*"
      ],
      "js": [
        "../../etf2l_match_page.ts"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/forum/user/*"
      ],
      "js": [
        "../../etf2l_player_page.ts"
      ],
      "css": [
        "../../styles.css"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://etf2l.org/teams/*"
      ],
      "js": [
        "../../etf2l_team_page.ts"
      ],
      "css": [
        "../../styles.css"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://logs.tf/*"
      ],
      "js": [
        "../../logstf.ts"
      ],
      "css": [
        "../../styles.css"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://steamcommunity.com/id/*"
      ],
      "js": [
        "../../steam_profile.ts"
      ]
    }
  ],
  "options_ui": {
    "page": "../../options.html"
  },
  "permissions": [
    "storage",
    "http://localhost/*",
    "*://offi.lemontea.dev/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{e8ce3e9f-e974-438c-b4e9-081c7ff7be34}"
    }
  },
  "web_accessible_resources": ["../../styles.css"]
}
